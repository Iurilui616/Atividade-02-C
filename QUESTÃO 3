#include <stdio.h>

int main() {
    int n1, n2, n3, n4, media;

    printf("Primeira nota: ");
    scanf("%d", &n1);

    printf("Segunda nota: ");
    scanf("%d", &n2);

    printf("Terceira nota: ");
    scanf("%d", &n3);

    printf("Quarta nota: ");
    scanf("%d", &n4);

    media = (n1 + n2 + n3 + n4) / 4;

    printf("A media do aluno e: %d\n", media);

    return 0;
}
